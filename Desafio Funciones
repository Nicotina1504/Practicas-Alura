function holaMundo () {
    console.log("hola mundo");
}

holaMundo();

function nombreX(nombre) {
    console.log(`Hola ${nombre}`);   
}

nombreX("Leonardo");

function dobleN (numero) {
    return(numero * 2)
}

let calculo = dobleN(5);
console.log(calculo);

function calcularPromedio(a,b,c) {
return (a+b+c) / 3;

}

let promedio = calcularPromedio(3,5,7);
console.log(promedio);

function mayorQue(a,b) {
    return a > b ? a : b;
}

let mayor = mayorQue(2,4);
console.log(mayor);

function siMismo(NumeroX) {
    return NumeroX * NumeroX;

}

let numeroCuadrado = siMismo(20);
console.log(numeroCuadrado);
